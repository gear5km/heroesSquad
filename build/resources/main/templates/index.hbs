    <title>Cover Template for Bootstrap</title>
    <!-- Bootstrap core CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.min.js"></script>
    <!-- Custom styles for this template -->
    <link href="css/cover.css" rel="stylesheet">
</head>
<body>
<div class="site-wrapper">
    <div class="site-wrapper-inner">
        <div id="carousel1" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100 carouselMain" alt="First slide" src="images/701805496_otrimmed.jpg" width="400px" height="600px">
                    <div class="carousel-caption d-none d-md-block" style="top:250px">
                        <h3>The Paras</h3>
                        <p>Arnhem 1944</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="images/eastAfrican.jpg" width="400px" height="600px" alt="Second slide">
                    <div class="carousel-caption d-none d-md-block" style="top:250px">
                        <h3>11th East African</h3>
                        <p>Burma 1945</p>
                    </div>
                </div>
                <a class="carousel-control-prev" href="#carousel1" role="button" data-slide="prev"> <span class="carousel-control-prev-icon" aria-hidden="true"></span> <span class="sr-only">Previous</span> </a>
                <a class="carousel-control-next" href="#carousel1" role="button" data-slide="next"> <span class="carousel-control-next-icon" aria-hidden="true"></span> <span class="sr-only">Next</span> </a>
            </div>
            <div class="cover-container">
                <div class="masthead clearfix">
                    <div class="inner">
                        <h3 class="masthead-brand">Heroes of the Second World War</h3>
                        <nav class="nav nav-masthead">
                            <a class="nav-link active" href="#">Home</a>
                            <a class="nav-link" href="#">Features</a>
                        </nav>
                    </div>
                </div>
                <div class="inner cover">
                    <h1 class="cover-heading Heading">Squad Up</h1>
                    <p class="lead">Get Your Troops, Your Gear and Your Guns</p>
                    <a href="#" class="btn btn-lg btn-secondary">Start</a>
                </div>
                <div class="mastfoot">
                </div>
            </div>
        </div>
    </div>
</div>
<footer class="w-100 py-4 flex-shrink-0" style="height:200px; color:grey">
    <p style="color:white">A Project by gear5km for Moringa School</p>
</footer>
